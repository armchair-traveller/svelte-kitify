{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$app/*": [".svelte/dev/runtime/app/*", ".svelte/build/runtime/app/*"],
			"$lib/*": ["src/lib/*"],
			"$stores/*": ["src/stores/*"],
			"$utils/*": ["src/utils/*"],
			"$gql/*": ["src/gql/*"]
		}
	},
	"include": ["src/**/*.js", "src/**/*.svelte"]
}
